{
	"name": "fasm-assembly",
	"displayName": "Fasm highlight",
	"description": "Fasm x86_64 assembly syntax highlighting",
	"version": "0.0.1",
	"publisher": "m_rgan",
	"preview": true,
	"engines": {
		"vscode": "^1.43.0"
	},
	"license": "MIT License",
	"categories": [
		"Programming Languages"
	],
	"author": {
		"name": "Pavel-from-Belarus"
	},
	"main": "./extension.js",
	"activationEvents": [
		"onCommand:fasm.initIndex"
	],
	"contributes": {
		"commands": [
			{
				"command": "fasm.initIndex",
				"title": "FASM: init index"
			}
		],
		"languages": [
			{
				"id": "fasm-x86_64",
				"aliases": [
					"fasm",
					"asm",
					"inc"
				],
				"extensions": [
					".fasm",
					".asm",
					".inc"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "fasm-x86_64",
				"scopeName": "source.asm_x86_64.fasm",
				"path": "./syntaxes/fasm-x86_64.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.74.0",
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"eslint": "^8.28.0",
		"glob": "^8.0.3",
		"mocha": "^10.1.0",
		"typescript": "^4.9.3",
		"@vscode/test-electron": "^2.2.0"
	},
	"dependencies": {
		"vscode": "^1.1.37"
	}
}
